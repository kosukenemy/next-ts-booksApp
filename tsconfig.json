{
    "compilerOptions": {
        "target": "es5",
        "lib": [
        "dom",
        "dom.iterable",
        "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx"
, "pages/_index.jss", "pages/_app.ts", "pages/components/Articles.tsx", "pages/components/Articles.tsx", "firebase/firebase.js"    ],
    "exclude": [
        "node_modules"
    ]
}